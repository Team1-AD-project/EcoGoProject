# Terraform Variables Example
# Rename to terraform.tfvars and fill in actual values

aws_region              = "us-east-1"
project_name            = "ecogo"
environment             = "staging"
vpc_cidr                = "10.0.0.0/16"
ecs_task_cpu            = "256"
ecs_task_memory         = "512"
ecs_service_desired_count = 1
ecr_repository_url      = "your-account-id.dkr.ecr.us-east-1.amazonaws.com/ecogo"
mongodb_uri             = "mongodb+srv://username:password@cluster0.mongodb.net/EcoGo"
